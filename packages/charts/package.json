{
  "name": "@beaubrain/chart-lib-charts",
  "version": "1.4.0",
  "description": "Chart implementations for charts library",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./line": {
      "types": "./dist/line/index.d.ts",
      "import": "./dist/line/index.mjs",
      "require": "./dist/line/index.js"
    },
    "./bar": {
      "types": "./dist/bar/index.d.ts",
      "import": "./dist/bar/index.mjs",
      "require": "./dist/bar/index.js"
    },
    "./pie": {
      "types": "./dist/pie/index.d.ts",
      "import": "./dist/pie/index.mjs",
      "require": "./dist/pie/index.js"
    },
    "./shared": {
      "types": "./dist/shared/index.d.ts",
      "import": "./dist/shared/index.mjs",
      "require": "./dist/shared/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "pnpm run clean && tsup && pnpm run build:dts",
    "build:dts": "tsc --project tsconfig.build.json --emitDeclarationOnly --declarationDir dist --rootDir src",
    "clean": "rm -rf dist *.tsbuildinfo",
    "dev": "tsup --watch"
  },
  "keywords": [
    "chart",
    "line-chart",
    "bar-chart",
    "pie-chart",
    "area-chart",
    "d3",
    "visualization",
    "data-visualization",
    "svg",
    "interactive-charts"
  ],
  "author": "TaeHyeon",
  "license": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/taehyeon3657/chartLibrary.git",
    "directory": "packages/charts"
  },
  "bugs": {
    "url": "https://github.com/taehyeon3657/chartLibrary/issues"
  },
  "homepage": "https://github.com/taehyeon3657/chartLibrary/tree/main/packages/charts#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@beaubrain/chart-lib-core": "^1.4.0",
    "@beaubrain/chart-lib-types": "^1.3.2"
  },
  "peerDependencies": {
    "d3": "^7.0.0"
  },
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "tsup": "^8.5.1",
    "typescript": "^5.0.0"
  },
  "gitHead": "528ac36b42331720695b5aa17d6ef7ebc7dca2e1"
}
