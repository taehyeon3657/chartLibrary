{
  "name": "@charts-library/charts",
  "version": "1.0.0",
  "description": "Chart implementations for charts library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./line": {
      "types": "./dist/line/index.d.ts",
      "import": "./dist/line/index.js",
      "require": "./dist/line/index.js"
    },
    "./bar": {
      "types": "./dist/bar/index.d.ts",
      "import": "./dist/bar/index.js",
      "require": "./dist/bar/index.js"
    },
    "./pie": {
      "types": "./dist/pie/index.d.ts",
      "import": "./dist/pie/index.js",
      "require": "./dist/pie/index.js"
    },
    "./shared": {
      "types": "./dist/shared/index.d.ts",
      "import": "./dist/shared/index.js",
      "require": "./dist/shared/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc --build",
    "dev": "tsc --build --watch",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "chart",
    "line-chart",
    "bar-chart",
    "pie-chart",
    "area-chart",
    "d3",
    "visualization",
    "data-visualization",
    "svg",
    "interactive-charts"
  ],
  "author": "TaeHyeon",
  "license": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/taehyeon3657/chartLibrary.git",
    "directory": "packages/charts"
  },
  "bugs": {
    "url": "https://github.com/taehyeon3657/chartLibrary/issues"
  },
  "homepage": "https://github.com/taehyeon3657/chartLibrary/tree/main/packages/charts#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@charts-library/core": "^1.0.0",
    "@charts-library/types": "^1.0.0"
  },
  "peerDependencies": {
    "d3": "^7.0.0"
  },
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "typescript": "^5.0.0"
  }
}