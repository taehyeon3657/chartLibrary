{
  "name": "@beaubrain/chart-lib-react",
  "version": "0.4.5",
  "description": "React components for charts library",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "pnpm run clean && pnpm run build:tsup",
    "build:tsup": "tsup src/index.tsx --format cjs,esm --dts --out-dir dist --splitting=false --target es2020 --tsconfig packages/react/tsconfig.build.json",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts,.tsx",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "react",
    "chart",
    "react-chart",
    "react-component",
    "d3",
    "visualization",
    "data-visualization",
    "line-chart",
    "bar-chart",
    "pie-chart",
    "hooks",
    "typescript"
  ],
  "author": "TaeHyeon",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "hhttps://github.com/taehyeon3657/chartLibrary.git",
    "directory": "packages/react"
  },
  "bugs": {
    "url": "https://github.com/taehyeon3657/chartLibrary/issues"
  },
  "homepage": "https://github.com/taehyeon3657/chartLibrary/tree/main/packages/react#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@beaubrain/chart-lib-charts": "^1.5.1",
    "@beaubrain/chart-lib-types": "^1.4.1"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.5.1",
    "typescript": "^5.0.2"
  },
  "gitHead": "528ac36b42331720695b5aa17d6ef7ebc7dca2e1"
}
